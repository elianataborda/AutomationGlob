<?xml version="1.0" encoding="UTF-8"?>

<suite name="Suite1" verbase="1">
	<test name="test1">
		<groups>
			<run>
				<include name="Smoke"></include>
				<include name="Regression"></include>
			</run>
		</groups>
		<classes>
			<class name="com.automation.training.Annotations">
				<methods>
					<include name="multiplicar"/>
					<include name="concatenacion"/>
				</methods>
			</class>
		</classes>
	</test>
	

</suite>
<suite name="Suite2" verbase="1">
	<test name="test2">
		<groups>
			<run>
				<include name="Smoke"></include>
				<exclude name="Regression"></include>
			</run>
		</groups>
		<classes>
			<class name="com.automation.training.Annotations">
				<methods>
					<include name="multiplicar"/>
					<include name="restar"/>
					<include name="fechadat"/>
				</methods>
			</class>
		</classes>
	</test>
	

</suite>